{
    "^!1": {
        "up": "^!{Tab}",
        "down": "#d",
        "left": "{Esc}",
        "right": "!+{Tab}",
        "default": "!{Tab}"
    },
    "^!2": {
        "up": "{Backspace}",
        "down": "#v",
        "left": "^x",
        "right": "^v",
        "default": "{Enter}"
    },
    "^!3": {
        "up": "{delete}",
        "down": "#{1}",
        "left": "^a",
        "right": "^c",
        "default": "#{2}"
    },
    "^!4": {
        "up": "fn:SCI",
        "down": "#+s",
        "left": "#{a}",
        "right": "^{y}",
        "default": "^{z}"
    },
    "^!5": {
        "up": "^{w}",
        "down": "^+{t}",
        "left": "",
        "right": "^+{Tab}",
        "default": "!{Right}"
    },
    "^!6": {
        "up": "",
        "down": "^{s}",
        "left": "",
        "right": "^{Tab}",
        "default": "!{Left}"
    },
    "^!7": {
        "up": "fn:TypeText",
        "down": "",
        "left": "",
        "right": "",
        "default": "{f5}"
    },
    "{mbutton}": {
        "up": "#{up}",
        "down": "#{down}",
        "left": "",
        "right": "#{right}",
        "default": "{rbutton}"
    },
    "{rbutton}": {
        "up": "",
        "down": "^c",
        "left": "",
        "right": "^v",
        "default": "{rbutton}"
    }
}
